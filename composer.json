{
  "name": "nitsan/ns-all-lightbox",
  "type": "typo3-cms-extension",
  "description": "Introducing the TYPO3 All-in-One Extension, a plugin that effortlessly integrates popular jQuery slider plugins into your website with just a few clicks! Enjoy a range of slider options including Nivo Slider, Royal Slider, Owlcarousel, Sliderjs Slider, and more.",
  "homepage": "https://t3planet.com/typo3-lightbox-extension",
  "license": "GPL-2.0+",
  "keywords": [
    "TYPO3 CMS",
    "Lightbox",
    "Fancybox",
    "Colorbox",
    "Prettyphoto",
    "Darkbox",
    "Magnific Popup",
    "T3Planet"
  ],
  "support": {
    "issues": "https://github.com/nitsan-technologies/ns_all_lightbox/issues"
  },
  "require": {
    "typo3/cms-core": "^7 || ^8  || ^9 || ^10  || ^11 || ^12 || ^13"
  },
  "autoload": {
    "psr-4": {
    }
  },
  "replace": {
    "typo3-ter/ns-all-lightbox": "self.version"
  },
  "config": {
    "vendor-dir": ".Build/vendor/",
    "bin-dir": ".Build/bin/"
  },
  "extra": {
    "typo3/cms": {
        "cms-package-dir": "{$vendor-dir}/typo3/cms",
        "web-dir": ".Build/public",
        "extension-key": "ns_all_lightbox"
    }
  }
}
